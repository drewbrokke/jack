{
	"compileOnSave": true,
	"compilerOptions": {
		"alwaysStrict": true,
		"lib": [
			"es2015",
			"es2016.array.include"
		],
		"module": "commonjs",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./dist",
		"pretty": true,
		"rootDir": "./src",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "es5"
	}
}